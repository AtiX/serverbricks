# EditorConfig is awesome: http://EditorConfig.org
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# Indent coffeescript, json with 2 spaces
[*.{coffee,json,yml,md,html,css}]
indent_style = space
indent_size = 2
max_line_length = 120
